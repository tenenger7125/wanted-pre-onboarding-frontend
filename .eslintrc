{
  "plugins": ["jest-dom", "testing-library"],
  "extends": ["react-app", "react-app/jest", "plugin:testing-library/react", "plugin:jest-dom/recommended"],
  "rules": {
    "testing-library/no-debugging-utils": [
      "warn",
      {
        "utilsToCheckFor": {
          "debug": true
        }
      }
    ]
  }
}
